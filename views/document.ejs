
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" href="../../favicon.ico">

    <title>Narrow Jumbotron Template for Bootstrap</title>

    <!-- Bootstrap core CSS -->
    
    
    
    <script src="//code.jquery.com/jquery-1.11.0.min.js"></script> 
    <link href="/css/bootstrap.min.css" rel="stylesheet">
    <script src="/js/bootstrap.min.js"></script>
    <link href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css" rel="stylesheet">
    <script src="/js/summernote.min.js"></script>
    <link href="/css/summernote.css" rel="stylesheet"/>
    <link href="/css/summernote-bs3.css" rel="stylesheet"/>
    <script src="/socket.io/socket.io.js"></script>





    <!-- Custom styles for this template -->
    <!-- <link href="jumbotron-narrow.css" rel="stylesheet">-->

    

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
  </head>

  <body>

    <div class="container">
      <div class="header">
        <ul class="nav nav-pills pull-right">
          <li class="active"><a href="#">Home</a></li>
          <li><a href="#">About</a></li>
          <li><a href="#">Contact</a></li>
        </ul>
        <h3 class="text-muted">Project name</h3>
      </div>

      <div class="jumbotron">
        <h1>Jumbotron heading</h1>
        
        <textarea id="summernote" class="form-control">Hello Summernote</textarea>

        <p><a class="btn btn-lg btn-success" href="#" role="button">Sign up today</a></p>
        <div  class="alert alert-warning">
            
            <div  id="message">
            </div>
        </div>
        <div  class="alert alert-warning">
            
            <div  id="documentid"><%= documentid %></div>
        </div>
      </div>



      <div class="footer">
        <p>&copy; Company 2014</p>
      </div>

    </div> <!-- /container -->


    <!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
    <script src="../../assets/js/ie10-viewport-bug-workaround.js"></script>
    
    
    <script>
     

    $('#summernote').summernote({
      height: 500,                 // set editor height

      minHeight: null,             // set minimum height of editor
      maxHeight: null,             // set maximum height of editor

      focus: true,                 // set focus to editable area after initializing summernote
    });

    

    </script>

    <script>
    var socket = io('/meetingdocument');

    var documentid = "";

    $(document).ready(function(){

      documentid = $('#documentid').text();

      console.log(documentid);

      socket.emit('join document',{documentid:documentid});

      socket.on('join document',function (data){

        $('#message').text("joined");



      });

     });
    </script>
  </body>
</html>
